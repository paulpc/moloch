{
  "node": true,
  "plusplus": false,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "latedef": "nofunc",
  "newcap": true,
  "nonew": true,
  "undef": true,
  "strict": true,
  "trailing": true,
  "esversion": 6,
  "unused": "vars",
